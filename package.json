{
  "name": "@apito-io/js-apito-plugin-sdk",
  "version": "0.1.0",
  "description": "JavaScript SDK for building Apito Engine HashiCorp plugins with gRPC support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "examples/"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "node build.js",
    "test": "jest",
    "lint": "eslint src --ext .js",
    "prepare": "npm run build",
    "example": "node examples/simple-plugin/main.js"
  },
  "author": "Apito Inc",
  "license": "MIT",
  "keywords": [
    "apito",
    "plugin",
    "graphql",
    "grpc",
    "hashicorp",
    "javascript",
    "nodejs",
    "sdk",
    "engine"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.8.0",
    "@grpc/proto-loader": "^0.7.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "eslint": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apito-io/engine.git",
    "directory": "js-apito-plugin-sdk"
  },
  "bugs": {
    "url": "https://github.com/apito-io/engine/issues"
  },
  "homepage": "https://github.com/apito-io/engine/tree/main/js-apito-plugin-sdk#readme"
}
